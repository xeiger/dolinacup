<!-- Content -->
<div id="content">
  <div class="container">

    <div class="row">
      <div class="col-md-12">

        <div class="space20"></div>

        <!-- Nav tabs -->
        <ul class="nav nav-tabs" role="tablist">
          <li role="presentation" class="active"><a href="#red" aria-controls="red" role="tab" data-toggle="tab">Červená skupina</a></li>
          <li role="presentation"><a href="#blue" aria-controls="blue" role="tab" data-toggle="tab">Modrá skupina</a></li>
        </ul>

        <!-- Tab panes -->
        <div class="tab-content">

          <div role="tabpanel" class="tab-pane active" id="red">
            <div class="space20"></div>

            <table class="table table-bordered">
                <tr class="result">
                  <th class="v-middle lightred" style="width: 5%;">#</th>
                  <th class="v-middle lightred" colspan="3" style="width: 50%;"></th>
                  <th class="v-middle lightred" stylpe="width: 15%">1. set</th>
                  <th class="v-middle lightred" style="width: 15%">2. set</th>
                  <th class="v-middle lightred" style="width: 15%">3. set</th>
                </tr>

                <% matches_red.each_with_index do |m, index| %>

                  <tr class="result">
                    <td style="width: 5%;"><%= index + 1 %></td>
                    <td style="width: 23%;" class="<%= 'lightred' if m[0] == 1 %>"><%= m[1] %></td>
                    <td style="width: 4%;">:</td>
                    <td style="width: 23%;" class="<%= 'lightred' if m[0] == 2 %>"><%= m[2] %></td>
                    <td style="width: 15%;"><%= m[3] %></td>
                    <td style="width: 15%;"><%= m[4] %></td>
                    <td style="width: 15%;"><%= m[5] %></td>
                  </tr>

                  <% end %>

            </table>

          </div>

          <div role="tabpanel" class="tab-pane" id="blue">
            <div class="space20"></div>

            <table class="table table-bordered">
                <tr class="result">
                  <th class="v-middle lightblue" style="width: 5%;">#</th>
                  <th class="v-middle lightblue" colspan="3" style="width: 50%;"></th>
                  <th class="v-middle lightblue" style="width: 15%">1. set</th>
                  <th class="v-middle lightblue" style="width: 15%">2. set</th>
                  <th class="v-middle lightblue" style="width: 15%">3. set</th>
                </tr>

                <% matches_blue.each_with_index do |m, index| %>

                  <tr class="result">
                    <td style="width: 5%;"><%= index + 1 %></td>
                    <td style="width: 23%;" class="<%= 'lightblue' if m[0] == 1 %>"><%= m[1] %></td>
                    <td style="width: 4%;">:</td>
                    <td style="width: 23%;" class="<%= 'lightblue' if m[0] == 2 %>"><%= m[2] %></td>
